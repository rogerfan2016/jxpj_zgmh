<%@ page language="java" contentType="text/html; charset=UTF-8"    pageEncoding="UTF-8"%><!DOCTYPE html><html><head><meta chaset="UTF-8"><%@ include file="/WEB-INF/pages/mobile/meta.jsp" %><title></title></head><body>    <script type="text/x-handlebars-template" id="amz-tpl">    <!-- {{>header header}} -->	<div class="content">		<div class="am-slider am-slider-default" data-am-flexslider id="demo-slider-0">		  <ul class="am-slides">			<li><img src="../img/wjdc/mobile/1.jpg" /></li>			<li><img src="../img/wjdc/mobile/2.jpg" /></li>			<li><img src="../img/wjdc/mobile/3.jpg" /></li>			<li><img src="../img/wjdc/mobile/4.jpg" /></li>		  </ul>		</div>	</div>    <div data-am-widget="tabs" class="am-tabs am-tabs-default">        <ul class="am-tabs-nav am-cf">		<c:choose>			<c:when test="${type eq 0 }">			<li class="am-active"><a onclick="showPanel('panel0');">我的信息</a></li>            <li class=""><a onclick="showPanel('panel1');">教学监控</a></li>            <li class=""><a onclick="showPanel('panel2');">教学评价</a></li>			</c:when>			<c:when test="${type eq 1 }">			<li class=""><a onclick="showPanel('panel0');">我的信息<span class="am-badge am-badge-warning am-round">5</span></a></li>            <li class="am-active"><a onclick="showPanel('panel1');">教学监控</a></li>            <li class=""><a onclick="showPanel('panel2');">教学评价</a></li>			</c:when>			<c:when test="${type eq 2 }">			<li class=""><a onclick="showPanel('panel0');">我的信息</a></li>            <li class=""><a onclick="showPanel('panel1');">教学监控</a></li>            <li class="am-active"><a onclick="showPanel('panel2');">教学评价</a></li>			</c:when>			<c:otherwise>			<li class="am-active"><a onclick="showPanel('panel0');">我的信息</a></li>            <li class=""><a onclick="showPanel('panel1');">教学监控</a></li>            <li class=""><a onclick="showPanel('panel2');">教学评价</a></li>			</c:otherwise>        </c:choose>            </ul>        <div id="panel0">            {{>gallery gallery0}}        </div>        <div id="panel1">            {{>gallery gallery1}}        </div>        <div id="panel2">            {{>gallery gallery2}}        </div>    </div></script><script type="text/javascript">     var $tpl = $('#amz-tpl');    var source = $tpl.text();    var template = Handlebars.compile(source);    data.header.content.title = "应用中心";    data.header.content.left = {};    var panel0 = [];    var panel1 = [];    var panel2 = [];    var gop = {};    gop.cols = 2;    gop.gallery = false;    data.gallery0.options = gop;    data.gallery0.theme = "bordered";    data.gallery1.options = gop;    data.gallery1.theme = "bordered";    data.gallery2.options = gop;    data.gallery2.theme = "bordered";        cobj = {};    cobj.img = "../assets/i/app/xxfj.png";    cobj.link = "../evaluation/mobile_pendingAffairList.html";    cobj.title = "待办事宜";    panel0.push(cobj);    cobj = {};    cobj.img = "../assets/i/app/xxls.png";    cobj.link = "";    cobj.title = "任务说明书";    panel0.push(cobj);    var cobj = {};    cobj.img = "../assets/i/app/msjs.png";    cobj.link = "../evaluation/mobile_curriculum.html";    cobj.title = "我的课表";    panel0.push(cobj);    cobj = {};    cobj.img = "../assets/i/app/xntz.png";    cobj.link = "../message/message_m_page.html";    cobj.title = "我的消息";    panel0.push(cobj);    data.gallery0.content = panel0;        cobj = {};    cobj.img = "../assets/i/app/msjs.png";    cobj.link = "../evaluation/mobileBusiness_center.html?btype=adjust_lesson_bid";    cobj.title = "调停课审核";    panel1.push(cobj);    cobj = {};    cobj.img = "../assets/i/app/ttxw.png";    cobj.link = "../evaluation/mobileBusiness_center.html?btype=school_change_bid";    cobj.title = "学籍异动审核";    panel1.push(cobj);    cobj = {};    cobj.img = "../assets/i/app/xntz.png";    cobj.link = "../monitor/mobile_patrol.html";    cobj.title = "教学巡视";    panel1.push(cobj);    cobj = {};    cobj.img = "../assets/i/app/xxdt.png";    cobj.link = "../evaluation/mobile_curriculumDetail.html";    cobj.title = "考勤管理";    panel1.push(cobj);    cobj = {};    cobj.img = "../assets/i/app/xxfj.png";    cobj.link = "";    cobj.title = "顶岗实习";    panel1.push(cobj);    data.gallery1.content = panel1;        cobj = {};    cobj.img = "../assets/i/app/msjs.png";    cobj.link = "../evaluation/mobile_response.html";    cobj.title = "学生评教";    panel2.push(cobj);    cobj = {};    cobj.img = "../assets/i/app/ttxw.png";    cobj.link = "../evaluation/mobile_myEvaluation.html";    cobj.title = "评教结果";    panel2.push(cobj);    cobj = {};    cobj.img = "../assets/i/app/xntz.png";    cobj.link = "../inspection_mobile/result_list.html";    cobj.title = "督导评教";    panel2.push(cobj);    cobj = {};    cobj.img = "../assets/i/app/xxdt.png";    cobj.link = "../evaluation/mobile_response2.html";    cobj.title = "教师评价";    panel2.push(cobj);    cobj = {};    cobj.img = "../assets/i/app/xxfj.png";    cobj.link = "";    cobj.title = "企业评价";    panel2.push(cobj);    data.gallery2.content = panel2;    var html = template(data);    $tpl.before(html);        function showPanel(id) {        $("#panel0").hide();        $("#panel1").hide();        $("#panel2").hide();        $("#" + id).show();    };    $(function () {    	if(${type }==0){    		$("#panel0").show();        	$("#panel1").hide();        	$("#panel2").hide();    	}else if(${type }==1){    		$("#panel0").hide();        	$("#panel1").show();        	$("#panel2").hide();    	}else if(${type }==2){    		$("#panel0").hide();        	$("#panel1").hide();        	$("#panel2").show();    	}else{    		$("#panel0").show();        	$("#panel1").hide();        	$("#panel2").hide();    	}          });</script><%@ include file="/WEB-INF/pages/mobile/navbar.jsp" %></body></html>